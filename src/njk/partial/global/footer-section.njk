<section
  id="footer__section"
  class="footer-section"
>
  <div class="container">
    <div class="footer-block">
      <div class="footer-block__head">
        <div class="footer-block__logo-col">
          <div class="footer-block__logo-img-field">
            <img
              src="{{ LAYOUT_CONFIG.imageUrl }}/logo/go2joy-logo.png?v={{ CACHE_VERSION }}"
              alt="Go2Joy"
              width="100%"
              height="100%"
              class="footer-block__logo-img"
            >
          </div> <!-- .footer-block__logo-img-field -->

          <div class="footer-block__slogan-field">
            <h2 class="footer-block__slogan-title-label">
              Đặt giờ, đặt đêm, đặt thêm ưu đãi
            </h2> <!-- .footer-block__slogan-title-label -->

            <div class="footer-block__slogan-content">
              Go2Joy là ứng dụng đặt phòng linh hoạt theo giờ, qua đêm hàng đầu tại Việt Nam
            </div> <!-- .footer-block__slogan-content -->
          </div> <!-- .footer-block__slogan-field -->
        </div> <!-- .footer-block__logo-col -->

        <div class="footer-block__smart-phone-img-col">
          <img
            src="{{ LAYOUT_CONFIG.imageUrl }}/go2joy-iphone-img.png?v={{ CACHE_VERSION }}"
            alt="Go2Joy application"
            width="100%"
            height="100%"
            class="footer-block__smart-phone-img lazyload"
          >
        </div> <!-- .footer-block__smart-phone-img-col -->

        <div class="footer-block__meta-info-col">
          <div class="footer-block__qrcode-img-col">
            <div class="footer-block__qrcode-img-outer">
              <div class="footer-block__qrcode-img-inner">
                <img
                  src="{{ LAYOUT_CONFIG.imageUrl }}/qrcode.png?v={{ CACHE_VERSION }}"
                  alt="Go2Joy"
                  width="100%"
                  height="100%"
                  class="footer-block__qrcode-img"
                >
              </div> <!-- .footer-block__qrcode-img-inner -->
            </div> <!-- .footer-block__qrcode-img-outer -->
          </div> <!-- .footer-block__qrcode-img-col -->

          <div class="footer-block__download-app-img-col">
            <div class="footer-block__download-app-img-outer">
              <a href="https://apps.apple.com/app/id1185445287?mt=8" class="footer-block__download-app-link" target="_blank" data-tracking-dable-key="TRACKING_VIEW_DOWNLOAD_IOS">
                <img
                  src="{{ LAYOUT_CONFIG.imageUrl }}/apple-download-btn.png"
                  alt="Go2Joy apple application"
                  width="100%"
                  height="100%"
                  class="footer-block__download-app-img"
                >
              </a>
            </div> <!-- .footer-block__download-app-img-outer -->

            <div class="footer-block__download-app-img-outer">
              <a href="https://play.google.com/store/apps/details?id=com.appromobile.hotel&hl=en&gl=US" class="footer-block__download-app-link" target="_blank" data-tracking-dable-key="TRACKING_VIEW_DOWNLOAD_ANDROID">
                <img
                  src="{{ LAYOUT_CONFIG.imageUrl }}/android-download-btn.png"
                  alt="Go2Joy android application"
                  width="100%"
                  height="100%"
                  class="footer-block__download-app-img"
                >
              </a>
            </div> <!-- .footer-block__download-app-img-outer -->
          </div> <!-- .footer-block__download-app-img-col -->
        </div> <!-- .footer-block__meta-info-col -->
      </div> <!-- .footer-block__head -->

      <div class="footer-block__body">
        <div class="footer-block__g2j-info-wrap">
          {% for go2joyInfoItem in go2joyInfoList %}
            <div class="footer-block__g2j-info-wrap-item">
              <div class="footer-block__g2j-info-wrap-item__head">
                {{ go2joyInfoItem.title }}
              </div> <!-- .footer-block__g2j-info-wrap-item__head -->

              <div class="footer-block__g2j-info-wrap-item__body">
                <ul class="footer-block__g2j-info-list">
                  {% for go2joyInfoDataItem in go2joyInfoItem.data %}
                    <li class="footer-block__g2j-info-item">
                      {% if not go2joyInfoDataItem.path | isEmpty %}
                        <a
                          href="{{ go2joyInfoDataItem.path }}"
                          class="enable-hover footer-block__g2j-info-item--link"
                          data-tracking-dable-key="{{ go2joyInfoDataItem.tracking_dable_key }}"
                        >
                          {{ go2joyInfoDataItem.title }}
                        </a>
                      {% elif not go2joyInfoDataItem.url | isEmpty %}
                        <a
                          class="enable-hover footer-block__g2j-info-item--link"
                          href="{{ go2joyInfoDataItem.url }}"
                          target="_blank"
                          data-tracking-dable-key="{{ go2joyInfoDataItem.tracking_dable_key }}"
                        >
                          {{ go2joyInfoDataItem.title }}
                        </a>
                      {% elif not go2joyInfoDataItem.phone | isEmpty %}
                        <span class="footer-block__g2j-info-label">
                          {{ go2joyInfoDataItem.title }}:
                        </span>

                        <a
                          class="footer-block__g2j-info-contact-label enable-hover footer-block__g2j-info-item--link"
                          href="tel: {{ go2joyInfoDataItem.phone }}"
                        >
                          {{ go2joyInfoDataItem.phone }}
                        </a>
                      {% elif not go2joyInfoDataItem.email | isEmpty %}
                        <span class="footer-block__g2j-info-label">
                          {{ go2joyInfoDataItem.title }}:
                        </span>

                        <a
                          class="footer-block__g2j-info-contact-label enable-hover footer-block__g2j-info-item--link"
                        >
                         {# href="mailto: {{ go2joyInfoDataItem.email }}" #}
                          {# {{ go2joyInfoDataItem.email }} #}
                        </a>
                      {% else %}
                        {{ go2joyInfoDataItem.title }}
                      {% endif %}
                    </li> <!-- .footer-block__g2j-info-item -->
                  {% endfor %}
                </ul> <!-- .footer-block__g2j-info-list -->
              </div> <!-- .footer-block__g2j-info-wrap-item__body -->
            </div> <!-- .footer-block__g2j-info-wrap-item -->
          {% endfor %}
        </div> <!-- .footer-block__g2j-info-wrap -->
      </div> <!-- .footer-block__body -->

      <div class="footer-block__footer">
        <div class="footer-block__follow-info-group">
          <div class="footer-block__follow-info-group__title">
            Theo dõi
          </div>

          <div class="footer-block__follow-info-group__follow-wrap">
            <a
              class="footer-block__follow-info-group__follow-item footer-block__follow-info-group__follow-item--yt enable-hover"
              href="https://www.youtube.com/channel/UCSDPkEcsKB9nJqc_fbWwSdw"
              target="_blank"
            >
              <i class="fab fa-youtube"></i>
            </a> <!-- .footer-block__follow-info-group__follow-item -->

            <a
              class="footer-block__follow-info-group__follow-item footer-block__follow-info-group__follow-item--fb enable-hover"
              href="https://www.facebook.com/go2joy.hotels"
              target="_blank"
            >
              <i class="fab fa-facebook-square"></i>
            </a> <!-- .footer-block__follow-info-group__follow-item -->
          </div> <!-- .footer-block__follow-info-group__follow-wrap -->
        </div> <!-- .footer-block__follow-info-group -->
      </div> <!-- .footer-block__footer -->
    </div> <!-- .footer-block -->
  </div> <!-- .container -->
</section> <!-- .footer-section -->
